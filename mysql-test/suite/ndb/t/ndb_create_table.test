-- source include/have_ndb.inc

create table t1 (a int primary key)
engine NDB
comment='NDB_TABLE=NOLOGGING';
show warnings;
drop table t1;

--disable_warnings
create table t1 (a int primary key)
engine NDB
comment='NDB_TABLE=NOLOGGING=1,NOLOGGING';
--enable_warnings
show warnings;
drop table t1;

--disable_warnings
create table t1 (a int primary key)
engine NDB
comment='NDB_TABLE=NOLOGGING=a,NOLOGGING';
--enable_warnings
show warnings;
drop table t1;

--disable_warnings
create table t1 (a int primary key)
engine NDB
comment='NDB_TABLE=NOLOGGING,NOLOGGINGa';
--enable_warnings
show warnings;
drop table t1;
