!include ../my.cnf

[cluster_config.slave2]
NoOfReplicas=                  1   
ndbd=
ndb_mgmd=
mysqld=


[mysqld.1.1]
server-id= 1
log-bin

[mysqld.1.slave]
server-id= 2
log-bin
skip-slave-start

[mysqld.1.slave2]
server-id= 3
log-bin
skip-slave-start

ndb_connectstring=	@mysql_cluster.slave.ndb_connectstring

[ENV]

SLAVE_MYPORT1=		@mysqld.1.slave2.port
SLAVE_MYSOCK1=		@mysqld.1.slave2.socket
