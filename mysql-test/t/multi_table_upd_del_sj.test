	       ON class.class_num=roster.class_num 
	       WHERE roster.class_num IN (SELECT class_num FROM roster);

let $queryd_2 = DELETE LOW_PRIORITY FROM class USING class INNER JOIN roster 
       	       ON class.class_num=roster.class_num 
